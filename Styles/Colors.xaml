  <SolidColorBrush x:Key="ContentBackgroundBrush" Color="{StaticResource WhiteColor}" options:Freeze="True" /> <!--original #FFFFFFFF-->
  <SolidColorBrush x:Key="ContentBorderBrush" Color="{StaticResource Gray7}" options:Freeze="True" /> <!--original #FFC8C8C8-->
  <SolidColorBrush x:Key="ContentForegroundBrush" Color="{StaticResource BlackColor}" options:Freeze="True" /> <!--original #FF333333-->
