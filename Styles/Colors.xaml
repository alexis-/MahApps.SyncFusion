<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"  
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    mc:Ignorable="options">

  <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource WhiteColor}" options:Freeze="True" />
  <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource Gray7}" options:Freeze="True" /> <!--original #FFC8C8C8-->
  <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource BlackColor}" options:Freeze="True" /> <!--original #FF333333-->
  <SolidColorBrush x:Key="GlyphBrush" Color="{StaticResource Gray2}" options:Freeze="True" /> <!--original #FF686969-->
  <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="{StaticResource WhiteColor}" options:Freeze="True" /> <!--original #FFFFFFFF-->
