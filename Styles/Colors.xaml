  <SolidColorBrush x:Key="ErrorLightBrush" Color="#FFEC777C" options:Freeze="True" /> <!--original #FFFF0000-->
  <SolidColorBrush x:Key="ActiveLightBackground" Color="{StaticResource Gray10}" options:Freeze="True" /> <!--original #FFA2D7EF-->
  <SolidColorBrush x:Key="ActiveDarkBackground" Color="{StaticResource Gray1}" options:Freeze="True" /> <!--original #FF0C4E6C-->
  <SolidColorBrush x:Key="ContentAlternativeBackground" Color="{StaticResource Gray7}" options:Freeze="True" /> <!--original #FFD8D8D8-->
  <SolidColorBrush x:Key="TickColor" Color="{StaticResource BlackColor}" options:Freeze="True" /> <!--original #FF333333-->
  <SolidColorBrush x:Key="RangeColor1" Color="#FF61A301" options:Freeze="True" /> <!--original #FF61A301-->
