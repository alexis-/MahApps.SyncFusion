  <SolidColorBrush x:Key="HeaderForegroundBrush" Color="{StaticResource BlackColor}" options:Freeze="True" /> <!--original #FF282827-->
  <SolidColorBrush x:Key="HeaderGlyphBrush" Color="{StaticResource Gray2}" options:Freeze="True" /> <!--original #FF686969-->
  <SolidColorBrush x:Key="HoverBackgroundBrush" Color="{StaticResource AccentColor3}" options:Freeze="True" /> <!--original #FFD1EAF5-->
